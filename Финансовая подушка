money_capital = 20000  # Подушка безопасности
salary = 5000  # Ежемесячная зарплата
spend = 6000  # Траты за первый месяц
increase = 0.05  # Ежемесячный рост цен

# TODO Посчитайте количество  месяцев, которое можно протянуть без долговв     #
months = 0
while money_capital > 0:

    money_capital -= spend


    if money_capital < 0:
        break
    money_capital += salary
    months += 1
    spend *= (1 + increase)


print(f"Вы сможете продержаться {months} месяцев.")
